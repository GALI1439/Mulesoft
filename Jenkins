pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn clean test'
            }
        }
        stage('Deploy') {
            steps {
                bat 'mvn deploy -DmuleDeploy'
            }
        }
    }
}
